app = "video2ascii-mp4-export"
primary_region = "iad"

[build]
  dockerfile = "deploy/mp4-server/Dockerfile"

[http_service]
  internal_port = 8080
  force_https = true
  auto_stop_machines = "stop"
  auto_start_machines = true
  min_machines_running = 0

[[vm]]
  size = "shared-cpu-1x"
  memory = "1024mb"
